{
  "name": "ssv445/aggregation-builder-pagination-bundle",
  "type": "symfony-bundle",
  "description": "Paginate Doctrine MongoDB ODM AggregationBuilder with KnpPaginatorBundle",
  "keywords": [
    "pager",
    "paginator",
    "pagination",
    "symfony",
    "bundle",
    "mongodb",
    "aggregation"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Shyam",
      "email": "shyam@readybytes.in"
    }
  ],
  "require": {
    "php": "^8.1",
    "knplabs/knp-paginator-bundle": "^5.9",
    "doctrine/mongodb-odm-bundle": "^4.5",
    "ext-mongodb": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpmd/phpmd": "^2.8",
    "phpstan/phpstan": "^0.12.18"
  },
  "autoload": {
    "psr-4": {
      "Ludo\\Bundle\\AggregationBuilderPaginationBundle\\": ""
    }
  },
  "config": {
    "sort-packages": true
  }
}